{
  "firebase": "sinanbolel",
  "public": "./public",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules",
    "**/bower_components",
    "public/src"
  ],
  "headers": [ {
    "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
    "headers" : [ {
      "key" : "Access-Control-Allow-Origin",
      "value" : "*"
    } ]
  }, {
    "source" : "**/*.@(jpg|jpeg|gif|png)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=7200"
    } ]
  } ],
  "redirects": [ {
    "source" : "/home",
    "destination" : "https://www.sinanbolel.com/",
    "type" : 301
  } , {
    "source" : "/contact",
    "destination" : "https://www.sinanbolel.com/#contact",
    "type" : 302
  } ]
}
