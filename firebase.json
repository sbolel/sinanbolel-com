{
  "firebase": "sinanbolel",
  "public": "./public",
  "ignore": [
    "firebase.json",
    "**/.*",
    "src",
    "release"
  ],
  "rules": "config/security-rules.json",
  "headers": [ {
    "source" : "**/*.@(jpg|jpeg|gif|png)",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=7200"
    } ]
  } ],
  "redirects": [ {
    "source" : "/home",
    "destination" : "https://www.sinanbolel.com/",
    "type" : 301
  } , {
    "source" : "/contact",
    "destination" : "https://www.sinanbolel.com/#contact",
    "type" : 302
  } ]
}
